; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "FSM",
"language": "GDScript",
"path": "res://src/scripts/fsm.gd"
}, {
"base": "Node",
"class": "FSM_State",
"language": "GDScript",
"path": "res://src/scripts/state.gd"
} ]
_global_script_class_icons={
"FSM": "",
"FSM_State": ""
}

[application]

config/name="Portal Bot"
run/main_scene="res://src/Main.tscn"
config/icon="res://sprites/robo/robo-icon.png"

[autoload]

actions="*res://src/redux/actions.gd"
action_types="*res://src/redux/action_types.gd"
initial_state="*res://src/redux/initial_state.gd"
reducers="*res://src/redux/reducers.gd"
store="*res://src/redux/store.gd"
Globals="*res://src/scripts/Globals.gd"
Constants="*res://src/scripts/Constants.gd"

[display]

window/size/width=160
window/size/height=90
window/size/test_width=960
window/size/test_height=540
mouse_cursor/custom_image="res://sprites/etc/brackeys-cursor.png"
window/stretch/mode="viewport"
window/stretch/aspect="keep"

